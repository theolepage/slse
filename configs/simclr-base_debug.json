{
    "name": "simclr-base_debug",
    "seed": 1717,
    "encoder": {
        "type": "ThinResNet34",
        "encoded_dim": 512,
        "weight_regularizer": 1e-4
    },
    "model": {
        "type": "SimCLR",
        "channel_loss_factor": 0.1,
        "weight_regularizer": 1e-4
    },
    "training": {
        "epochs": 50,
        "batch_size": 256,
        "learning_rate": 0.0001,
        "dataset": {
            "type": "LibriSpeech",
            "sample_frequency": 16000,
            "train_paths": [
                "./datasets/LibriSpeech/dev-clean/*"
            ],
            "val_ratio": 0.2,
            "test_ratio": 0.1,
            "frames": {
                "pick": "sequence",
                "length": 20480,
                "stride": 20480,
                "count": 1
            }
        }
    }
}