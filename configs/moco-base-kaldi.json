{
    "name": "moco-base-kaldi",
    "seed": 1717,
    "encoder": {
        "type": "XVector",
        "encoded_dim": 3000,
        "weight_regularizer": 1e-4
    },
    "model": {
        "type": "MoCo",
        "queue_size": 10000,
        "info_nce_temp": 0.07,
        "embedding_dim": 512,
        "proto_nce_loss_factor": 0.25,
        "nb_clusters": 5000,
        "clustering_negs_count": 10000,
        "epochs_before_proto_nce": 1111111111111,
        "weight_regularizer": 1e-4
    },
    "training": {
        "epochs": 150,
        "optimizer": {
            "type": "SGD",
            "momentum": 0.9
        },
        "batch_size": 1024,
        "learning_rate": {
            "scheduler": "cosine",
            "start": 0.1,
            "end": 0.0001
        },
        "dataset": {
            "type": "Kaldi",
            "sample_frequency": 16000,
            "scp": "./data/train/feats.scp",
            "utt2spk": "./data/train/utt2spk",
            "frames": {
                "length": 300,
                "extract_mfcc": true
            }
        }
    }
}